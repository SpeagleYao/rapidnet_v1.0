prov_r1_3 eknow(@Local, A, B, S, RID, RWeight, RLOC) :- eknowTemp(@RLOC, Local, A, B, S, RID, RWeight, R, List).
prov_r1_4 know(@Local, A, B, S) :- eknow(@Local, A, B, S, RID, RWeight, RLOC).
prov_r1_5 prov(@Local, VID, RID, RLOC, Score) :- eknow(@Local, A, B, S, RID, RWeight, RLOC), VID := f_sha1((((know+ Local)+ A)+ B)+ S), Score := -1.
prov_r2_1 eknowTemp(@RLOC, Local, A, B, S, RID, RWeight, R, List) :- like(@Local, A, C), PID1 := f_sha1(((like+ Local)+ A)+ C), List := f_append(PID1), like(@Local, B, C), PID2 := f_sha1(((like+ Local)+ B)+ C), List2 := f_append(PID2), List := f_concat(List, List2), A != B, S := 1, RLOC := Local, RWeight := 0.4, R := r2, RID := f_sha1((R+ RLOC)+ List).
prov_r3_1 eknowTemp(@RLOC, Local, A, B, S, RID, RWeight, R, List) :- know(@Local, A, C, S1), PID1 := f_sha1((((know+ Local)+ A)+ C)+ S1), List := f_append(PID1), know(@Local, B, C, S2), PID2 := f_sha1((((know+ Local)+ B)+ C)+ S2), List2 := f_append(PID2), List := f_concat(List, List2), A != B, S1 == 1, S2 == 1, S := 1, RLOC := Local, RWeight := 1, R := r3, RID := f_sha1((R+ RLOC)+ List).
prov_r4_1 eknowTemp(@RLOC, Local, A, B, S, RID, RWeight, R, List) :- relation(@Local, A, B), PID1 := f_sha1(((relation+ Local)+ A)+ B), List := f_append(PID1), A != B, S := 0, RLOC := Local, RWeight := 0.2, R := r4, RID := f_sha1((R+ RLOC)+ List).
prov_r5_1 eknowTemp(@RLOC, Local, A, B, S, RID, RWeight, R, List) :- live(@Local, A, C1), PID1 := f_sha1(((live+ Local)+ A)+ C1), List := f_append(PID1), live(@Local, B, C2), PID2 := f_sha1(((live+ Local)+ B)+ C2), List2 := f_append(PID2), List := f_concat(List, List2), C1 != C2, A != B, S := 0, RLOC := Local, RWeight := 0.8, R := r5, RID := f_sha1((R+ RLOC)+ List).
prov_edb_1 prov(@Local, VID, RID, RLoc, Score) :- RLoc := Local, like(@Local, A, C), VID := f_sha1(((like+ Local)+ A)+ C), RID := VID, Score := C.
prov_edb_2 prov(@Local, VID, RID, RLoc, Score) :- RLoc := Local, live(@Local, A, C), VID := f_sha1(((live+ Local)+ A)+ C), RID := VID, Score := C.
prov_edb_3 prov(@Local, VID, RID, RLoc, Score) :- RLoc := Local, relation(@Local, A, B), VID := f_sha1(((relation+ Local)+ A)+ B), RID := VID, Score := B.
edb1 pReturn(@Ret, QID, VID, Prov) :- provQuery(@X, QID, VID, Ret), prov(@X, VID, RID, RLoc, Score), RID == VID, Prov := f_pEDB(VID, X).
idb1 pQList(@X, QID, LIST< RID >) :- provQuery(@X, QID, VID, Ret), prov(@X, VID, RID, RLoc, Score), RID != VID.
idb2 pResultTmp(@X, QID, Ret, VID, Buf) :- provQuery(@X, QID, VID, Ret), Buf := f_empty().
idb3 pIterate(@X, QID, N) :- provQuery(@X, QID, VID, Ret), N := 1.
idb4 pIterate(@X, QID, N) :- pIterate(@X, QID, N1), pQList(@X, QID, List), N1 < f_size(List), N := N1+ 1.
idb5 eRuleQuery(@X, NQID, RID) :- pIterate(@X, QID, N), pQList(@X, QID, List), N <= f_size(List), RID := f_item(List, N), NQID := f_sha1((+ QID)+ RID).
